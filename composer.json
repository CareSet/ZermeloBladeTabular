{
    "name": "careset/zermelobladetabular",
    "description": "Zermelo, a PHP reporting engine for Laravel. The Tabular Reporting Engine",
    "type": "library",
    "license": "Apache-2.0",
     "keywords": [
                "laravel",
                "reporting",
                "reports",
                "report",
                "datatables",
                "d3",
                "vega"
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4" : {
            "CareSet\\ZermeloBladeTabular\\" : "src/"
        }
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "CareSet\\ZermeloBladeTabular\\ServiceProvider"
	        ]
	    }
	},
     "config": {
        "sort-packages": true
    }
}
